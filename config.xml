<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.comparahora.app" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
  <name>Comparahora</name>
  <description>
    Comparahora Admin App
  </description>
  <author email="kuamatzin@gmail.com" href="http://comparahora.com/">
    Carlos Cuamatzin Hernandez
  </author>
  <content src="index.html"/>
  <access origin="*"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="android-minSdkVersion" value="16"/>
  <preference name="BackupWebStorage" value="none"/>
  <gap:plugin name="com.pushbots.push" version="1.2.3" source="plugins.cordova.io" />
  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true"/>
  </feature>
</widget>